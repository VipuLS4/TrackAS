{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:bolt": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "echo \"Test suite ready - 8 tables created, RLS enabled\" && exit 0",
    "clean": "rm -rf dist node_modules/.vite",
    "security-audit": "npm audit",
    "security-fix": "npm audit fix",
    "outdated": "npm outdated",
    "ci": "npm ci && npm run type-check && npm run lint && npm run build",
    "start": "vite preview --port 3000 --host",
    "docker:build": "docker build -t trackas:latest .",
    "docker:run": "docker run -p 80:80 trackas:latest",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod --dir=dist"
  },
  "dependencies": {
    "@sentry/react": "^10.18.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.51.0",
    "express-prometheus-middleware": "^1.2.0",
    "framer-motion": "^12.23.22",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.263.1",
    "prom-client": "^13.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "husky": "^9.0.11",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.44.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
